{
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  },
  "rules": {
     "no-console": [0, ""],
     "no-underscore-dangle": "off",
     "arrow-body-style": "off"
  }
}
